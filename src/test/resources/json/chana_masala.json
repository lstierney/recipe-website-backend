  "methodSteps": [
    {
      "ordering": 1,
      "description": "Fry the bay leaf, cloves, cinnamon and cardamom until fragrant"
    },
    {
      "ordering": 2,
      "description": "Add onion. Continue to cook until light golden"
    },
    {
      "ordering": 3,
      "description": "Add the garlic/ginger paste and cook until raw smell goes away (a couple of mins)"
    },
    {
      "ordering": 4,
      "description": "Add the tomatoes and salt a cook for a few more mins (until heated through and bubbling)"
    },
    {
      "ordering": 5,
      "description": "Add the red chilli powder, turmeric, garam masala, coriander powder, asafoetida and cumin powder. Cook until aromatic (about 5 mins)"
    },
    {
      "ordering": 7,
      "description": "Remove the bay leaf and cinnamon and blend this mixture until smooth."
    },
    {
      "ordering": 8,
      "description": "Add the chick peas and some chick pea stock/water and cook for a while (doesn't really matter)"
    },
    {
      "ordering": 9,
      "description": "Just before serving stir in the fenugreek leaves and mango powder"
    }
  ],
